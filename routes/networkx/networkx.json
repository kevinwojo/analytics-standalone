{
	"form-action": "/networkx",
	"containers":[
		{"container-name":"file-container",
		"container-label-name":"select from your folder",
		"inputs":[
					{"type":"select",
						"name":"selectFile",
						"id":"selectFile",
						"sytle":"",
						"class":"form-control",
						"optgroup": true
					}
				]},
		{"container-name":"",
		"container-label-name":"network relation",
		"inputs":[
					{"type":"select",
						"name":"relationships",
						"id":"relationships",
						"class":"form-control",
						"optgroup": false,
						"options":[
							{"value":"reply_to",
							"label":"reply to"},
							
							{"value":"retweet_from",
							"label":"retweet from"}						
						]
					}
				]},
		{"container-name":"",
		"container-label-name":"select graph layout",
		"inputs":[
					{"type":"select",
						"name":"layout",
						"id":"layout",
						"class":"form-control",
						"optgroup": false,
						"options":[
							{"value":"random",
							"label":"random layout"},
							
							{"value":"circular",
							"label":"circular layout"},
							
							{"value":"fruchterman",
							"label":" fruchterman layout"},
							
							{"value":"spring",
							"label":" spring layout"},
							
							{"value":"shell",
							"label":" shell layout"},
							
							{"value":"spectral",
							"label":" spectral layout"}
							
						]
					}
				]},
		{"container-name":"",
		"container-label-name":"remove isolate pairs",
		"inputs":[
					{"type":"radio",
						"name":"prune",
						"id":"false",
						"value":"false",
						"sytle":"position: absolute; visibility: hidden;",
						"class":"customized-radio",
						"checked":true,
						"label":"No"
					},
					{"type":"radio",
						"name":"prune",
						"id":"true",
						"value":"true",
						"sytle":"position: absolute; visibility: hidden;",
						"class":"customized-radio",
						"checked":false,
						"label":"Yes"
					}
				]}
	],
	"custom-script":"../bootstrap/js/customized/view_networkx.js"
}
			